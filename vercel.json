{
  "version": 2,
  "github": {
    "silent": true
  },
  "public": true,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.ts"
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/demo", "destination": "/index.html" }
  ]
}
