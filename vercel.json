{
  "version": 2,
  "github": {
    "silent": true
  },
  "public": true,
  "builds": [{ "src": "/(.*)" }],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.ts"
    }
  ]
}
